    "#如果參數有預設值,預設值一定要放在最後面\n",
    "def menu(wine,entree,dessert=\"奶昔\"):\n",
    "    print(f\"wine={wine}\")\n",
    "    print(f\"entree={entree}\")\n",
    "    print(f\"dessert={dessert}\")"
   ]
